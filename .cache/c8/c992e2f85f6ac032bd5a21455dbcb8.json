cument.getElementById('root'));\r\n    }\r\n\r\n    render(){\r\n        return(\r\n            <div>\r\n                <h2>Books List</h2>\r\n                <table align=\"center\">\r\n                    <tbody>\r\n                        <tr>\r\n                            <th>Book Name</th>\r\n                            <th>ISBN</th>\r\n                            <th>Author</th>\r\n                            <th>Price</th>\r\n                            <th>Year</th>\r\n                            <th>Publisher</th>\r\n                        </tr>\r\n                        {this.state.books}\r\n                    </tbody>\r\n                        <button onClick={this.addBook} >Add New Book</button>\r\n                </table>\r\n            </div>\r\n        )\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"59aa8a9f661611f13df3da335a0c44c4","cacheData":{"env":{}}}